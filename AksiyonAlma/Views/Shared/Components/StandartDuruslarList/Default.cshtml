@using Entity.Concrete
@model List<StandartDuruslar>

<h4>Standart Duruşlar</h4>
<table class="table table-bordered">


    <thead>

        <tr>
            <th>Başlangıç</th>
            <th>Bitiş</th>
            <th>Duruş Nedeni</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>@item.Baslangic.ToString("HH:mm")</td>
                <td>@item.Bitis.ToString("HH:mm")</td>
                <td>@item.DurusNedeni</td>

            </tr>
        }
    </tbody>
</table>
